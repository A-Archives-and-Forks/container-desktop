{
  "editor.suggestSelection": "first",
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "all",
  "files.eol": "\n",
  "files.exclude": {
    "**/__pycache__/**": true,
    "**/.git/**": true,
    "**/.DS_Store": true,
    "**/*.egg-info": true,
    ".pytest_cache": true
  },
  "files.watcherExclude": {
    "**/coverage/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/__pycache__/**": true,
    "**/.git/**": true,
    "**/.docker/**": true,
    "**/.venv/**": true,
    "**/.DS_Store": true,
    ".pytest_cache": true
  },
  "files.associations": {
    "*.env": "ini",
    "*.env.local": "ini",
    "*.runtime": "ini",
    "*.mk": "makefile",
    "docker-compose.override.yml.template": "dockercompose"
  },
  "search.exclude": {
    "**/coverage/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/.git/**": true,
    "**/__pycache__/**": true,
    "**/.venv/**": true,
    "**/.DS_Store": true,
    ".pytest_cache": true,
    "yarn.lock": true,
    "package.lock": true
  },
  "cSpell.words": [
    "aquasec",
    "aquasecurity",
    "autofocus",
    "autoos",
    "blkio",
    "blueprintjs",
    "browserslist",
    "buildah",
    "Callout",
    "cgroupfs",
    "devel",
    "devpts",
    "draggable",
    "electronjs",
    "flathub",
    "flatpak",
    "freedesktop",
    "GEOSEARCH",
    "groupinstall",
    "Healthcheck",
    "hookform",
    "icns",
    "iife",
    "immer",
    "inited",
    "iongion",
    "Ionut",
    "IPAM",
    "ipvlan",
    "istoica",
    "jsonpath",
    "kernelmode",
    "Kube",
    "libpod",
    "limactl",
    "localdomain",
    "macvlan",
    "markdownlint",
    "Minimizable",
    "Mountpoint",
    "noconflict",
    "noheading",
    "noninteractive",
    "npipe",
    "nsis",
    "nvmrc",
    "osascript",
    "pacman",
    "peasy",
    "percpu",
    "pids",
    "Pletosu",
    "podman",
    "podstate",
    "portmappings",
    "precpu",
    "preread",
    "procs",
    "proxying",
    "pulseaudio",
    "rcedit",
    "Resetable",
    "scrollback",
    "Stoica",
    "svgr",
    "systeminfo",
    "tabindex",
    "Teodora",
    "themelink",
    "TLDR",
    "tmpfs",
    "trivy",
    "usehooks",
    "usermode",
    "uuidv",
    "vite",
    "vitejs",
    "whereis",
    "wslg"
  ],
  "jshint.enable": false,
  "yaml.customTags": ["!Secret scalar", "!Env scalar"],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": true
  },
  "search.followSymlinks": false,
  "python.formatting.provider": "black",
  "python.languageServer": "Default"
}
