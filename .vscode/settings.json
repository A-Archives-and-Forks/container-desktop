{
  "[html]": {
    "editor.defaultFormatter": "j69.ejs-beautify"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "always",
      "source.fixAll.eslint": "always",
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "always",
      "source.fixAll.eslint": "always",
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "always",
      "source.fixAll.eslint": "always",
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml",
    "editor.formatOnSave": true
  },
  "biome.enabled": false,
  "cSpell.ignorePaths": ["src/web-app/assets/languages.json"],
  "cSpell.words": [
    "ALLUSERS",
    "aquasec",
    "aquasecurity",
    "asar",
    "axios",
    "blkio",
    "blueprintjs",
    "browserslistrc",
    "buildcheck",
    "CGROUPSV",
    "cppvsdbg",
    "csstools",
    "CURRENTUSER",
    "deepmerge",
    "devel",
    "devpts",
    "dotnettools",
    "Editability",
    "electronjs",
    "elfutils",
    "flathub",
    "flatpak",
    "freedesktop",
    "GEOSEARCH",
    "groupinstall",
    "Healthcheck",
    "HKLM",
    "hookform",
    "icns",
    "icnsutils",
    "immer",
    "Indo",
    "inited",
    "inkscape",
    "inno",
    "iongion",
    "Ionut",
    "IPAM",
    "ipvlan",
    "ISSH",
    "Japonic",
    "kernelmode",
    "keystyle",
    "libpod",
    "limactl",
    "listoption",
    "macvlan",
    "Mergeable",
    "Minimizable",
    "Mountpoint",
    "mscdex",
    "mstile",
    "Netcat",
    "netns",
    "nodemon",
    "npipe",
    "nsis",
    "nvmrc",
    "originjs",
    "osascript",
    "pacman",
    "pbkdf",
    "peasy",
    "percpu",
    "pids",
    "Pletosu",
    "podstate",
    "portmappings",
    "precpu",
    "preread",
    "printenv",
    "procs",
    "Proxying",
    "pulseaudio",
    "pyenv",
    "Resetable",
    "resty",
    "resvg",
    "rimraf",
    "rootfull",
    "RUNTIMES",
    "scrollback",
    "slirp",
    "sonoma",
    "statelyai",
    "Stoica",
    "stylelint",
    "svgo",
    "svgr",
    "tauri",
    "Teodora",
    "TLDR",
    "tmpfs",
    "trivy",
    "unplugin",
    "usehooks",
    "usermode",
    "webgl",
    "whereis",
    "winget",
    "wslg",
    "xattr",
    "xstate"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always",
    "source.fixAll.stylelint": "always",
    "source.fixAll.tslint": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.enable": true,
  "eslint.options": {},
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "files.associations": {
    "*.env": "ini",
    "*.env.**": "ini",
    "*.mk": "makefile",
    "*.runtime": "ini"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git/**": true
  },
  "files.watcherExclude": {
    "**/.DS_Store": true,
    "**/.git/**": true,
    "**/build/**": true,
    "**/release/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": ["./src/web-app/translations"],
  "prettier.documentSelectors": ["*.{tsx,ts}", "*.{jsx,js}"],
  "search.exclude": {
    "**/.DS_Store": true,
    "**/.git/**": true,
    "**/build/**": true,
    "**/release/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    ".eslintcache": true
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "git.ignoreLimitWarning": true
}
